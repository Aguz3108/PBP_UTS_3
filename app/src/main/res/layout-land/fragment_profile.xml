<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    tools:context=".ProfileActivity">

    <!-- TODO: Update blank fragment layout -->

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="283dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="170px"
            android:background="@drawable/action_bar_style"
            app:layout_constraintStart_toStartOf="parent">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="15dp"
                android:gravity="center"
                android:text="Profil"
                android:textColor="#fff"
                android:textSize="16pt" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="300dp"
            android:layout_height="430px"
            android:layout_marginTop="160px"
            android:gravity="center"
            android:layout_marginLeft="200dp">



            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:id="@+id/image_view"
                android:background="@drawable/rent" />
        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/categories_box_style">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="350dp"
            android:layout_marginTop="20dp"
            android:background="@drawable/profile_box_style">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="85dp">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="17.5dp"
                    android:layout_marginLeft="15dp"
                    android:background="@drawable/ic_profile"
                    android:backgroundTint="#000"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="7.5dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginRight="400dp"
                    android:text="Company :     HokiRental"
                    android:textColor="#fff"
                    android:textSize="8pt"
                    android:gravity=""/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_marginTop="45dp">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="20.5dp"
                    android:background="@drawable/ic_calendar"
                    android:backgroundTint="#000" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="7.5dp"
                    android:layout_marginLeft="75dp"
                    android:layout_marginRight="400dp"
                    android:text="Built          :     Sept 20, 2020"
                    android:textColor="#fff"
                    android:textSize="8pt"
                    android:gravity=""/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_marginTop="0dp"
                android:layout_marginBottom="10dp">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginLeft="370dp"
                    android:background="@drawable/ic_person"
                    android:backgroundTint="#000"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="7.5dp"
                    android:layout_marginLeft="420dp"
                    android:text="Founder  :    Joseph Maria A.Y.W"
                    android:textColor="#fff"
                    android:textSize="8pt"
                    android:gravity=""/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="85dp"
                android:layout_marginTop="45dp">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="7.5dp"
                    android:layout_marginLeft="370dp"
                    android:background="@drawable/ic_location"
                    android:backgroundTint="#000"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="7.5dp"
                    android:layout_marginLeft="420dp"
                    android:text="Address  :    Babarsari street,"
                    android:textColor="#fff"
                    android:textSize="8pt"
                    android:gravity=""/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="70dp"
                    android:layout_marginTop="45dp"
                    android:layout_marginLeft="200dp"
                    android:text="                                                                       Yogyakarta, Indonesia"
                    android:textColor="#fff"
                    android:textSize="8pt"
                    android:gravity=""/>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>

</FrameLayout>